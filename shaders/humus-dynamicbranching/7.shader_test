[require]
GLSL >= 1.10

[fragment shader]
#version 130
#define saturate(x) clamp(x,0.0,1.0)
#define lerp mix
#line 13
uniform sampler2D Base;

varying vec2 texCoord;

void main(){
	gl_FragColor = texture2D(Base, texCoord) * gl_Color;
}






































[vertex shader]
#version 130
#define saturate(x) clamp(x,0.0,1.0)
#define lerp mix
#line 2
varying vec2 texCoord;

void main(){
	gl_Position = ftransform();
	texCoord = gl_MultiTexCoord0.xy;
	gl_FrontColor = gl_Color;
}



