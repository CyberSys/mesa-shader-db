}
vec4 ConstColorProcessor_Stage2_c1_c0(vec4 _input) {
    vec4 _output;
    {
        _output = ucolor_Stage2_c1_c0;
    }
    return _output;
}
vec4 blend_src_in(vec4 src, vec4 dst) {
    return src * dst.w;
}
vec4 blend_dst_in(vec4 src, vec4 dst) {
    return blend_src_in(dst, src);
}
void main() {
    vec4 outputColor_Stage0;
